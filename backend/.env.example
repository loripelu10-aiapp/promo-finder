# PromoFinder Environment Variables
# Copy this file to .env and fill in your values

# Server
PORT=3000
NODE_ENV=production

# Admin Password (change this!)
ADMIN_PASSWORD=your_secure_password_here

# Awin Affiliate Network
AWIN_PUBLISHER_ID=your_awin_publisher_id
AWIN_API_KEY=your_awin_api_key

# TradeDoubler Affiliate Network
TRADEDOUBLER_ORG_ID=your_tradedoubler_org_id
TRADEDOUBLER_TOKEN=your_tradedoubler_token

# Amazon Associates
AMAZON_PARTNER_TAG=promofinder-21
AMAZON_ACCESS_KEY=your_amazon_access_key
AMAZON_SECRET_KEY=your_amazon_secret_key
