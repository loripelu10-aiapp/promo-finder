{
  "name": "promo-finder-backend",
  "version": "1.0.0",
  "description": "Backend API for PromoFinder",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "db:generate": "cd .. && npx prisma generate",
    "db:migrate": "cd .. && npx prisma migrate dev",
    "db:migrate:deploy": "cd .. && npx prisma migrate deploy",
    "db:seed": "cd .. && npx ts-node prisma/seed.ts",
    "db:studio": "cd .. && npx prisma studio",
    "db:reset": "cd .. && npx prisma migrate reset",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "axios": "^1.6.2",
    "cheerio": "^1.1.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "deepl-node": "^1.12.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^8.1.0",
    "node-cron": "^3.0.3",
    "papaparse": "^5.5.3",
    "pg": "^8.16.3",
    "prisma": "^7.2.0",
    "puppeteer": "^24.34.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "redis": "^4.6.12",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "jest": "^30.2.0",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
